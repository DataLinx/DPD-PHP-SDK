{
    "name": "datalinx/dpd-php-sdk",
    "description": "PHP SDK for DPD Slovenia and Croatia",
    "keywords": [
        "dpd",
        "easyship",
        "api",
        "webservice",
        "php",
        "sdk",
        "slovenia",
        "slovenija",
        "croatia",
        "hrvatska"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Omer Šabić",
            "email": "omer@datalinx.si"
        }
    ],
    "homepage": "https://github.com/datalinx/dpd-php-sdk",
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "DataLinx\\DPD\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DataLinx\\DPD\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --allow-risky=yes"
    }
}
